          <values>
            <value id="tcp">tcp</value>
            <value id="udp">udp</value>
          </values>
          <default_value>tcp</default_value>
          <is_null_allowed>true</is_null_allowed>
        </field>
        <field id="port" xsi:type="AttributeInteger">
